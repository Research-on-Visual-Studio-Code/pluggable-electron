{
  "name": "pluggable-electron",
  "version": "0.4.0",
  "description": "A framework to create a plugin API in Electron apps",
  "keywords": [
    "electron",
    "plugin",
    "framework",
    "extend",
    "extension"
  ],
  "main": "pluginMgr/index.js",
  "module": "dist/execution.es.js",
  "preload": "facade/index.js",
  "pluginMgr": "pluginMgr/index.js",
  "types": "index.d.ts",
  "files": [
    "facade/*",
    "pluginMgr/*",
    "dist/*"
  ],
  "scripts": {
    "docs": "node docs/genDocs.js",
    "build": "rollup --config",
    "types": "tsc"
  },
  "author": "Igor Honhoff",
  "repository": "github:dutchigor/pluggable-electron",
  "license": "MIT",
  "dependencies": {
    "is-electron-renderer": "^2.0.1",
    "pacote": "^13.0.4"
  },
  "devDependencies": {
    "jsdoc-to-markdown": "^7.1.1",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "electron": ">= 13.x < 18"
  },
  "bugs": {
    "url": "https://github.com/dutchigor/pluggable-electron/issues"
  }
}